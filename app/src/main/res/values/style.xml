<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextViewHeading">
        <item name="android:fontFamily">@font/meidum</item>
        <item name="android:textSize">25sp</item>
    </style>

    <style name="InputEdittext">
        <item name="android:padding">@dimen/_10dp</item>
        <item name="android:drawableTint">@color/dove_gray</item>
        <item name="android:background">@drawable/custom_input</item>
        <item name="android:textAppearance">@font/regular</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="LongButton">
        <item name="android:fontFamily">@font/bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>

    </style>
</resources>